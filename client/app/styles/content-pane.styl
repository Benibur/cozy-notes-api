
// Notes 

#editor
    overflow hidden
    margin 0 
    margin-left 320px
    padding 10px
    height auto
    background-color bgcoloreditor

.note-full
    background #333333
    shadow 2px 2px 5px bgshadowcolor 
    border 1px solid notebordercolor
    padding 10px
    border-radius 3px
    height 90%
    margin-bottom 10px

.note-full h2
    padding 0
    margin-bottom 20px

#note-full-content
    width 98%
    
#note-full-content-with-video
    display inline-block
    margin 10px
    width 48%
    
#note-full-content-with-video:hover
    border 1px solid blue
