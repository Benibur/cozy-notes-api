
// Notes 

#editor
    margin 0 
    margin-left 320px
    padding 10px
    height auto
    background-color bgcoloreditor
    
#note-area
    position absolute
    bottom 10px
    top 100px
    right 15px
    left 15px
    
#note-area td
    position absolute
    top 45px
    bottom 10px
    left 0px
    width 100%
    
#note-full-breadcrumb
    margin-left 10px
    color #006ED8

#note-full-breadcrumb a
    color #49A1F6

.note-full
    background #333333
    shadow 5px 5px 5px bgshadowcolor 
    border-radius 3px
    position absolute
    top 10px
    bottom 10px
    right 10px
    left 10px

.note-full h2
    color #B5B5B5
    padding 0
    margin-bottom 20px
    margin-left 20px

#titleBtn   
    color #333333
    font-weight bold
