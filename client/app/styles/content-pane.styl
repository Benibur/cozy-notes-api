
// Notes 

#editor
    margin 0 
    margin-left 320px
    padding 10px
    height auto
    background-color bgcoloreditor
    
#note-area
    position absolute
    bottom 10px
    top 65px
    right 15px
    left 15px
    
#note-area td
    position absolute
    top 45px
    bottom 10px
    left 0px
    width 100%
    
#note-full-breadcrumb
    margin-left 10px
    color #006ED8

#note-full-breadcrumb a
    color #49A1F6

.note-full
    background #333333
    shadow 5px 5px 5px bgshadowcolor 
    border-radius 3px
    position absolute
    top 10px
    bottom 10px
    right 10px
    left 10px

.note-full h2
    color #B5B5B5
    padding 0
    position absolute
    top 20px
    left 10px
    right 10px
    
.note-full h2:focus
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(82, 168, 236, 0.6)
    border-color: rgba(82, 168, 236, 0.8)
    outline: 0 none
    
#titleBtn   
    color #333333
    font-weight bold
