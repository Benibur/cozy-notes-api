<div class="page-header">
    <h1>New note</h1>
</div>

<% form_for(note, {action: path_to.notes, method: 'POST', id: "note_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('notes/form.ejs', {locals: {form: form, note: note}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create note', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.notes, {class: 'btn'}) %>
    </div>
<% });%>
